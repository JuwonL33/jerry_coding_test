import datetime

def solution(a, b):
    arr = ['SUN', 'MON', 'TUE', 'WED', 'THU', 'FRI', 'SAT']
    start = 5
    start_date, end_date = datetime.datetime(2016,1,1), datetime.datetime(2016,a,b)
    diff = end_date - start_date
    return arr[(start+diff.days)%7]

print(solution(5, 24))
