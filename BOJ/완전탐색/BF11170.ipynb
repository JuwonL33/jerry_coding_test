   "source": [
    "# 0의 개수\n",
    "# https://www.acmicpc.net/problem/11170\n",
    "n = int(input())\n",
    "for _ in range(n) : \n",
    "    start, end = map(int, input().split())\n",
    "    cnt = 0\n",
    "    for i in range(start, end+1) : \n",
    "        tmp = str(i)\n",
    "        cnt += tmp.count('0')\n",
    "    print(cnt)"
   ]
  }
