    "# 모든 순열\n",
    "# https://www.acmicpc.net/problem/10974\n",
    "\n",
    "from itertools import *\n",
    "\n",
    "arr = [ i+1 for i in range(int(input())) ]\n",
    "\n",
    "for i in permutations(arr, len(arr)) : \n",
    "    for a in i : \n",
    "        print(a, end = ' ')\n",
    "    print()"
   ]
