    "# 완전탐색\n",
    "# 약수 구하기\n",
    "# https://www.acmicpc.net/problem/2501\n",
    "\n",
    "n, k = map(int, input().split())\n",
    "arr = [0, 1]\n",
    "\n",
    "for i in range(2, n+1) : \n",
    "    if n % i == 0 : \n",
    "        arr.append(i)\n",
    "\n",
    "print(arr)\n",
    "\n",
    "if len(arr) > k : print(arr[k])\n",
    "else : print(0)\n",
    "\n"
   ]
  }
