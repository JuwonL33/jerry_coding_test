    "# 분해합\n",
    "# https://www.acmicpc.net/problem/2231\n",
    "\n",
    "n = int(input())\n",
    "result = 0\n",
    "for i in range(1, n+1) : \n",
    "    a = list(map(int, str(i)))\n",
    "    result = i + sum(a)\n",
    "    if result == n : \n",
    "        print(i)\n",
    "        break\n",
    "    \n",
    "    if i == n : \n",
    "        print(0)"
