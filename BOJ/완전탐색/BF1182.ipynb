   "source": [
    "# 부분 수열의 합\n",
    "# https://www.acmicpc.net/problem/1182\n",
    "\n",
    "from itertools import * \n",
    "\n",
    "n, s = map(int, input().split())\n",
    "arr = list(map(int, input().split()))\n",
    "cnt = 0\n",
    "for i in range(1, n+1) : \n",
    "    for comb in combinations(arr, i) : \n",
    "        if sum(comb) == s : cnt += 1\n",
    "\n",
    "print(cnt)\n"
   ]
  }
