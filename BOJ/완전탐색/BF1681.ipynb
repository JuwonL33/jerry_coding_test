    "# 줄 세우기\n",
    "# https://www.acmicpc.net/problem/1681\n",
    "\n",
    "N, L = input().split() \n",
    "N = int(N) \n",
    "cnt = 0\n",
    "n = 0\n",
    "while cnt != N : \n",
    "    n += 1\n",
    "    if L in str(n) : \n",
    "        continue\n",
    "    cnt += 1\n",
    "print(n)"
   ]
  }
