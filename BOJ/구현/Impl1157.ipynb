   "source": [
    "# 단어 공부\n",
    "# https://www.acmicpc.net/problem/1157\n",
    "\n",
    "string = input().upper()\n",
    "counter = list(set(string))\n",
    "\n",
    "cnt = []\n",
    "for i in counter : \n",
    "    count = string.count\n",
    "    cnt.append(count(i))\n",
    "\n",
    "if cnt.count(max(cnt)) > 1 : \n",
    "    print(\"?\")\n",
    "else : \n",
    "    print(counter[(cnt.index(max(cnt)))])\n",
    "\n",
    "\n"
   ]
  }
