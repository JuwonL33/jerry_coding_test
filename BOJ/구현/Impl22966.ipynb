   "source": [
    "# 가장 쉬운 문제를 찾는 문제\n",
    "# https://www.acmicpc.net/problem/22966\n",
    "\n",
    "N = int(input())\n",
    "dic = {}\n",
    "\n",
    "for _ in range(N) : \n",
    "    title, level = map(str, input().split())\n",
    "    dic[title] = int(level)\n",
    "\n",
    "result = sorted(dic.items(), key=lambda x:x[1])\n",
    "print(result[0][0])"
   ]
  }
