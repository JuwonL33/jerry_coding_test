   "source": [
    "# 첼시를 도와줘!\n",
    "# https://www.acmicpc.net/problem/11098\n",
    "\n",
    "T = int(input())\n",
    "for _ in range(T) :\n",
    "    t = int(input())\n",
    "    di = dict()\n",
    "    for _ in range(t) : \n",
    "        money, name = map(str, input().split())\n",
    "        di[name] = int(money)\n",
    "    print(max(di, key=di.get))"
   ]
  }
