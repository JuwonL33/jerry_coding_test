    "# 애너그램 만들기\n",
    "# https://www.acmicpc.net/problem/1919\n",
    "\n",
    "a = list(input())\n",
    "b = list(input())\n",
    "\n",
    "a1, b1 = a[:], b[:]\n",
    "rslt1 = [x for x in a if not x in b1 or b1.remove(x)]\n",
    "rslt2 = [x for x in b if not x in a1 or a1.remove(x)]\n",
    "\n",
    "print(len(rslt1) + len(rslt2))"
   ]
  }
