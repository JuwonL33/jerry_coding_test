    "# 알파벳 개수\n",
    "# https://www.acmicpc.net/problem/10808\n",
    "\n",
    "s = input()\n",
    "arr = [ 0 for _ in range(26) ]\n",
    "\n",
    "for a in s : \n",
    "    arr[ord(a)-97] = s.count(a)\n",
    "\n",
    "for a in arr : \n",
    "    print(a, end=' ')"
   ]
