   "source": [
    "# 영수증\n",
    "# https://www.acmicpc.net/problem/25304\n",
    "\n",
    "tot = int(input())\n",
    "cnt = int(input())\n",
    "sum_amt = 0\n",
    "for _ in range(cnt) : \n",
    "    a, b = map(int, input().split())\n",
    "    sum_amt += a*b\n",
    "\n",
    "if sum_amt == tot : \n",
    "    print(\"Yes\")\n",
    "else : \n",
    "    print(\"No\")"
   ]
  }
