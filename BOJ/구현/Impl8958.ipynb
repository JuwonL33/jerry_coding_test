   "source": [
    "# OX퀴즈\n",
    "# https://www.acmicpc.net/problem/8958\n",
    "\n",
    "n = int(input())\n",
    "for _ in range(n) : \n",
    "    sum = 0\n",
    "    arr = input().split(\"X\")\n",
    "    for a in arr : \n",
    "        if len(a) == 0 : continue\n",
    "        else : \n",
    "            for i in range(len(a)) : \n",
    "                sum += i+1\n",
    "    print(sum)"
   ]
  }
