# 그룹 단어 체커
# https://www.acmicpc.net/problem/1316

t = int(input())
arr = []
for _ in range(t) : 
    arr.append(input())
    
cnt = 0
for a in arr : 
    tmp = []
    status = 1
    for i in range(len(a)) : 
        if a[i] not in tmp : 
            tmp.append(a[i])
        elif tmp[-1] == a[i] :
            continue
        else : 
            status = 0
    if status == 1 : cnt += 1
        
print(cnt)

