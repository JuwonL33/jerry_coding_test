    "# 세제곱의 합\n",
    "# https://www.acmicpc.net/problem/28701\n",
    "\n",
    "N = int(input())\n",
    "a = N*(N+1)//2\n",
    "b = a*a\n",
    "c = 0\n",
    "for i in range(1, N+1) :  \n",
    "    c += i**3\n",
    "print(a)\n",
    "print(b)\n",
    "print(c)"
   ]
  }
