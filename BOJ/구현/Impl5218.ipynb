# 알파벳 거리
# https://www.acmicpc.net/problem/5218

T = int(input())

for _ in range(T) : 
    x, y = map(str, input().split())
    # ord()
    result = []
    for i in range(len(x)) : 
        if ord(y[i]) - ord(x[i]) >= 0 : 
            result.append(str(ord(y[i]) - ord(x[i])))
        else : 
            result.append(str(ord(y[i])+26 - ord(x[i])))

    print("Distances:", end = ' ')
    for a in result :
        print(a, end = ' ')
    print()
