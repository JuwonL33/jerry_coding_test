    "# 이항 계수 1\n",
    "# https://www.acmicpc.net/problem/11050\n",
    "\n",
    "def factorial(n) : \n",
    "    ans = 1\n",
    "    for i in range(2, n+1) : \n",
    "        ans *= i\n",
    "    return ans\n",
    "\n",
    "n, k = map(int, input().split())\n",
    "\n",
    "result = factorial(n) // factorial(k) // factorial(n-k)\n",
    "print(result)"
   ]
  }
