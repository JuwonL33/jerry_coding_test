# 행렬 덧셈
# https://www.acmicpc.net/problem/2738

n, m = map(int, input().split())

matrix_A = [ list(map(int, input().split())) for i in range(n) ]
matrix_B = [ list(map(int, input().split())) for i in range(n) ]
    
for i in range(n) : 
    for j in range(m) : 
        print(matrix_A[i][j]+matrix_B[i][j], end= ' ')
    print("")
