    "# 하샤드 수\n",
    "\n",
    "def solution(x):\n",
    "    if x > 9 : \n",
    "        tmp = list(map(int, str(x)))\n",
    "        if x % sum(tmp) == 0 : return True\n",
    "        else : return False\n",
    "    else : return True"
   ]
