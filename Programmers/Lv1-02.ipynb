    "# 직사각형 별찍기\n",
    "\n",
    "a, b = map(int, input().strip().split(' '))\n",
    "for i in range(b) : \n",
    "    for j in range(a) : \n",
    "        print(\"*\", end=\"\")\n",
    "    print(\"\")\n"
