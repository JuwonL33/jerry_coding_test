   "source": [
    "# 부족한 금액 구하기\n",
    "# 문제를 잘 읽어야 함\n",
    "# ~한 상황에서는 어떻게 해야 하는지에 대한 처리 확실히 할 것\n",
    "\n",
    "def solution(price, money, count):\n",
    "    answer = 0\n",
    "    for i in range(1, count+1) : \n",
    "        answer += price * i\n",
    "    \n",
    "    if money-answer > 0 : return 0\n",
    "    else : return abs(money-answer)"
   ]
