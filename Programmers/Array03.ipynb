    "from itertools import permutations\n",
    "\n",
    "def solution(numbers):\n",
    "    numbers2 = list(map(str, numbers))\n",
    "    numbers2.sort(key=lambda x: x*3, reverse=True)\n",
    "    answer = str(int(''.join(numbers2)))\n",
    "    return answer\n",
    "\n",
    "solution([6, 10, 2])"
   ]
